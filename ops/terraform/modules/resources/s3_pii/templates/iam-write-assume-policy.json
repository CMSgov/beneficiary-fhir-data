{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "BFDPIIS3WriteAssume",
      "Effect": "Allow",
      "Action": [
        "s3:ListBucket*",
        "s3:GetObject*",
        "s3:PutObject*",
        "s3:DeleteObject*"
      ],
      "Resource": [
        "arn:aws:s3:::${bucket_id}",
        "arn:aws:s3:::${bucket_id}/*"
      ]
    }
  ]
}