			<!-- Just useful. -->
			<groupId>com.google.guava</groupId>
			<artifactId>guava</artifactId>
			<version>24.1.1-jre</version>
		</dependency>

		<!-- Provide the api and implementation for XML binf which are used by many BFD modules. -->
